# Firebase Configuration
# Get these values from your Firebase project settings
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google AI API Configuration (for AI chat features)
# Get from Google AI Studio: https://aistudio.google.com/app/apikey
REACT_APP_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Razorpay Configuration (for payment features)
# Get from Razorpay Dashboard: https://dashboard.razorpay.com/app/keys
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id_here
REACT_APP_RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here

# Development Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG_MODE=true