{
  "name": "infoscape-electron",
  "version": "2.0.0",
  "description": "InfoScape - Advanced OSINT Intelligence Toolkit",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "start-safe": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder",
    "preelectron-pack": "npm run build",
    "dist": "npm run build && electron-builder",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "dev-safe": "concurrently \"npm run start-safe\" \"wait-on http://localhost:3000 && electron .\""
  },
  "keywords": [
    "osint",
    "intelligence",
    "investigation",
    "social-media",
    "people-search",
    "cybersecurity",
    "electron",
    "react"
  ],
  "author": "Ivo Pereira",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.2",
    "@mui/x-date-pickers": "^6.18.2",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "electron-store": "^8.1.0",
    "lodash": "^4.17.21",
    "plotly.js": "^2.27.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.19.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "vis-network": "^9.1.6",
    "web-vitals": "^3.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.ivocreates.infoscape",
    "productName": "InfoScape",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
